{
  "git.ignoreLimitWarning": true,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "jestrunner.jestCommand": "yarn test",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.nyc_output": true,
    "**/build": true,
    "**/node_modules": true,
    "**/coverage": true,
    ".vscode": true,
    ".gradle": true,
    "test-reports": true
  }
}
